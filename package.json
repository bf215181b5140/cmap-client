{
  "name": "cmap2-client",
  "version": "0.1.0",
  "private": true,
  "main": "build/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "tsc": "tsc",
    "electron": "electron .",
    "build": "react-scripts build && npm run tsc",
    "start": "npm run build && electron .",
    "package": "npm run build && npx electron-packager . cmap2 --ignore=^src$ --ignore=^public$ --ignore=^node_modules$ --ignore=^.gitignore$ --ignore=^package-lock.json$ --ignore=^README.md$ --ignore=^tsconfig"
  },
  "dependencies": {
    "electron": "^21.2.3",
    "electron-store": "^8.1.0",
    "node-osc": "^8.0.6",
    "remixicon": "^2.5.0",
    "socket.io-client": "^4.5.3",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/node": "^16.18.3",
    "@types/node-osc": "^6.0.0",
    "@types/react": "^18.0.25",
    "@types/styled-components": "^5.1.26",
    "@types/electron": "^1.6.10",
    "@types/react-dom": "^18.0.9",
    "electron-packager": "^17.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0",
    "react-router-dom": "6.4.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
