{
  "name": "cmap2-client",
  "version": "0.1.0",
  "private": true,
  "main": "build/electron/electron.js",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "package": "npm run build && npx electron-packager . cmap2 --ignore=^src$ --ignore=^public$ --ignore=^node_modules$ --ignore=^docs$ --ignore=^.gitignore$ --ignore=^package-lock.json$ --ignore=^README.md$ --ignore=^tsconfig.json --ignore=^vite.config.js"
  },
  "dependencies": {
    "node-osc": "^8.0.6",
    "remixicon": "^2.5.0",
    "socket.io-client": "^4.5.3"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^16.18.3",
    "@types/node-osc": "^6.0.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^3.0.0",
    "electron": "^21.2.3",
    "electron-packager": "^17.1.1",
    "electron-store": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0",
    "react-router-dom": "6.4.3",
    "react-select": "^5.7.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.8.4",
    "vite": "^4.0.1",
    "vite-electron-plugin": "^0.5.2",
    "vite-plugin-electron-renderer": "^0.11.3"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777"
    }
  }
}
